{
  "compileOnSave": true,
  "exclude": [
    "src/types"
  ],
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es6",
      "dom"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "baseUrl": "src/types",
    "typeRoots": [
      "node_modules/@types",
      "src/types"
    ],
    "outDir": "build"
  }
}